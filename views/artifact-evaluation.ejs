    <body class="d-flex h-100 text-center text-white bg-dark">
        <div class="container d-flex w-100 h-100 p-3 mx-auto flex-column">
            
            <%- include('common/header'); -%>
            
            <main class="pt-5 px-3">
                <h1>Artifact Evaluation</h1>
                <p class="lead">Use the options below to build your artifact for evaluation</p>
                <div class="row row-cols-1 row-cols-lg-12 align-items-stretch g-4 py-3">
                    <div class="col-4">
                      <div class="card border-light h-100 overflow-hidden text-white bg-dark rounded-5">
                        <div class="d-flex flex-column h-100 p-5 text-white">
                          <h1 class="mt-3 mb-3 display-6 lh-1 fw-bold">NAH</h1>
                          <h2 class="mb-3 display-6 lh-1">Users Registered</h2>
                        </div>
                      </div>
                    </div>
                    <div class="col-8">
                        <div class="card border-light h-100 overflow-hidden text-white bg-dark rounded-5">
                          <div class="row row-cols-2">
                            <div class="col">
                              <div class="d-flex flex-column h-100 p-5 text-white">
                                <h1 class="mt-3 mb-3 display-6 lh-1 fw-bold">Set</h1>
                              </div>
                            </div>
                          <div class="col">
                            <div class="dropdown dropdown-dark border-0 pt-0 mx-0 rounded-3">
                              <button class="btn btn-dark btn-outline-light dropdown-toggle" type="button" id="setDropdownButton" data-bs-toggle="dropdown" aria-expanded="false">Pick a Set <span class="caret"></span></span></button>
                              <ul class="dropdown-menu dropdown-menu-dark mb-0" aria-labelledby="setDropdownButton" id="setDropdownItems">
                                  <input onkeyup="setInputKey()" type="text" class="form-control form-control-dark" id="setInput" autocomplete="false" placeholder="Type to filter...">
                                <% sets.forEach(set => { %>
                                <li><a class="dropdown-item align-items-center gap-2 py-1">
                                  <%= set.set_name %>
                                </a></li>
                                <% }) %>
                              </ul>
                            </div>
                          </div>
                      </div>
                        <div class="row row-cols-2">
                            <div class="col">
                              <div class="d-flex flex-column h-50 p-2 text-white">
                                <h1 class="mt-3 mb-3 display-6 lh-1 fw-bold">Type</h1>
                              </div>
                            </div>
                            <div class="col">
                              <div class="d-flex flex-column h-50 p-2 text-white">
                                <div class="btn-group" role="group" aria-label="Basic outlined example">
                                  <% types.forEach(type => { %>
                                    <button type="button" class="btn btn-outline-light"><%= type._id %></button>
                                  <% }) %>
                                </div></h1>
                              </div>
                            </div>
                      </div>
                      </div>
                </div>
            </main>
            <%- include('common/footer'); -%>
        </div>
